#!/bin/sh



if [[ $2 == prod ]]

then

echo 'Downloading data...'

wget --content-disposition -nv http://htsstation.epfl.ch/pipeline_builder/jobs/download?key=$1

elif [[ $2 == dev ]]

then

echo 'Downloading data...'

wget --content-disposition -nv http://dev.htsstation.vital-it.ch/pipeline_builder/jobs/download?key=$1

else

echo 'USAGE: ./htsstation_results [HTSSTATION_KEY] [dev|prod]'

exit 0

fi

